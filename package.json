{
  "name": "stencil-realworld-example-app",
  "version": "0.0.1",
  "description": "Stencil RealWorld Example App (Conduit)",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@stencil/core": "^1.0.0-alpha.8",
    "@stencil/router": "^0.3.3",
    "marked": "^0.6.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/puppeteer": "1.12.3",
    "husky": "^1.3.1",
    "jest": "24.7.1",
    "jest-cli": "24.7.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "puppeteer": "1.14.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-stencil": "^1.0.0",
    "workbox-build": "4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
